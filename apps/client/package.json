{
  "name": "bl1nk-skill-ide",
  "productName": "bl1nk Skill IDE",
  "version": "0.1.0",
  "description": "AI Skill IDE - Like VS Code but for AI development. Create agents, skills, MCP tools, knowledgebases, and memory systems.",
  "main": ".vite/build/main.js",
  "repository": {
    "type": "git",
