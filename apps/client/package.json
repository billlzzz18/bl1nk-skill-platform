  "scripts": {
    "clean": "rimraf out .vite node_modules",
    "start": "electron-forge start",
    "dev": "electron-forge start",
    "build": "rimraf out .vite && electron-forge package",
    "package": "npm run clean && electron-forge package",
    "make": "npm run clean && electron-forge make",
    "publish": "npm run clean && electron-forge publish",
    "ts": "npx tsc -p tsconfig.json --noEmit",
    "lint": "npx oxlint --fix",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "prettier": "npx prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "ai",
    "skill",
    "ide",
    "agents",
    "mcp",
    "claude",
    "gpt",
    "llm"
