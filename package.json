{
  "name": "claude-skill-builder",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel -r dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:e2e": "playwright test",
    "test:unit": "pnpm -r test:unit",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "pnpm -r type-check",
    "validate": "pnpm run format:check && pnpm run lint && pnpm run type-check",
    "validate:skills": "node scripts/validators/validate-skills.js",
    "validate:agents": "node scripts/validators/validate-agents.js",
    "health": "node scripts/health-check.js",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.3.0",
    "typescript": "^5.6.0",
    "@modelcontextprotocol/sdk": ">=1.24.0"
  },
  "dependencies": {
    "sdk": "file:specs\\main\\generated\\sdk",
    "@modelcontextprotocol/sdk": ">=1.24.0"
  }
}
